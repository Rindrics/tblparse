{
	"name": "@rindrics/tblparse",
	"version": "0.0.5",
	"description": "Table block detection library for Excel/CSV files",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Rindrics/muramasa"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "tsc --project tsconfig.build.json",
		"lint": "biome lint src",
		"format": "biome format src --write",
		"check": "biome check src",
		"test": "bun test",
		"prepare": "husky"
	},
	"keywords": [
		"table",
		"parser",
		"excel",
		"csv",
		"xlsx",
		"spreadsheet",
		"block-detection",
		"data-extraction"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"bun-types": "^1.3.5",
		"husky": "^9.1.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"xlsx": "^0.18.5"
	}
}
