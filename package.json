{
	"name": "tblparse-demo",
	"version": "0.0.1",
	"type": "module",
	"description": "Table parsing library monorepo",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "bun run --cwd apps/demo dev",
		"build": "bun run --cwd packages/core build && bun run --cwd apps/demo build",
		"lint": "biome lint .",
		"format": "biome format . --write",
		"check": "biome check .",
		"test": "bun run --cwd packages/core test",
		"prepare": "husky"
	},
	"keywords": [
		"table",
		"parser",
		"excel",
		"csv",
		"xlsx",
		"spreadsheet",
		"block-detection",
		"data-extraction"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"bun-types": "^1.3.5",
		"husky": "^9.1.7",
		"typescript": "^5.9.3"
	}
}
